<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dominio.TipoEquipoComputo" table="TipoEquipoComputo" >
        <id name="idTipoEquipoComputo" type="java.lang.Integer">
            <column name="id_tipoEquipoComputo" />
            <generator class="identity" />
        </id>
        <property name="valorCaracteristica" type="java.lang.Integer">
            <column name="valor_caracteristica"  not-null="true" />
        </property>
        
        <property name="etiquetaTipoEquipo" type="java.lang.String">
            <column name="etiqueta_tipoEquipo" length="45" not-null="true" />
        </property>
        <property name="descTipoEquipo" type="java.lang.String">
            <column name="desc_tipoEquipo" length="45" />
        </property>
        
        
        <set name="equipoComputos" inverse="true">
            <key>
                <column name="id_tipoEquipo" not-null="true" />
            </key>
            <one-to-many class="dominio.EquipoComputo" />
        </set>
        <set name="caracteristicaEquipos" table="TipoCaracteristicaEquipo" >
            <key>
                <column name="id_tipoEquipoComputo" not-null="true" />
            </key>
            <many-to-many entity-name="dominio.CaracteristicaEquipo">
                <column name="id_caracterisrica_equipo" not-null="true" />
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>
